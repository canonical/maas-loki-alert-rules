name: MAASLxdhostOverprovisioned
expr: |
  rate(
    {job=~"maas-audit",maas_module="drivers.pod.lxd"}
    |~ "Not enough //S+"
    [1m]
  )
for: 0m
labels:
  severity: warning
annotations:
  summary: "LXD Host is over provisioned"
  description: "A LXD Host has too many VMs provisioned for its resources."
